{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ES6",
    "jsx": "react-jsx",
    "rootDir": "./src",
    "outDir": "./dist",
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowImportingTsExtensions": true,
    "noEmit":true,
    
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}
